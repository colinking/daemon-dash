{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "Experimental boilerplate code.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run prod",
    "dev": "NODE_ENV=development nodemon index.js --exec babel-node",
    "prod": "NODE_ENV=production node index.js",
    "build": "NODE_ENV=production webpack -p",
    "postinstall": "npm run build",
    "watch": "NODE_ENV=development webpack --watch",
    "clean": "rm -rf ./dist/",
    "test": "mocha \"client/**/*.test.js\" \"server/**/*.test.js\"",
    "lint": "eslint --ext .js --ext .jsx index.js client server"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MrPickles/boilerplate.git"
  },
  "author": "Andrew Liu",
  "dependencies": {
    "compression": "^1.6.2",
    "css-modules-require-hook": "^4.0.5",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "mongoose": "^4.7.6",
    "pug": "^2.0.0-beta6",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "semantic-ui-css": "^2.2.4",
    "semantic-ui-react": "^0.63.5",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.1.1",
    "nodemon": "^1.11.0",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-node-externals": "^1.5.4"
  }
}
